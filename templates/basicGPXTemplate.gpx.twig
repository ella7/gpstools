<?xml version="1.0" encoding="UTF-8"?>
<gpx creator="{{ creator }}" version="1.1" xmlns="http://www.topografix.com/GPX/1/1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.topografix.com/GPX/1/1 http://www.topografix.com/GPX/1/1/gpx.xsd http://www.garmin.com/xmlschemas/GpxExtensions/v3 http://www.garmin.com/xmlschemas/GpxExtensionsv3.xsd http://www.garmin.com/xmlschemas/TrackPointExtension/v1 http://www.garmin.com/xmlschemas/TrackPointExtensionv1.xsd">
 <metadata>
  <time>{{ track.track_points[0].modifiedTimeString }}</time>
 </metadata>
  <trk>
    <name>{{ track_name }}</name>
    <trkseg>
      {% for track_point in track.track_points %}
<trkpt lat="{{ track_point.coord.latitude }}" lon="{{ track_point.coord.longitude }}">
        <ele>{{ track_point.altitude }}</ele>
        <time>{{ track_point.modifiedTimeString }}</time>
      </trkpt>
      {% endfor %}
</trkseg>
  </trk>
</gpx>
